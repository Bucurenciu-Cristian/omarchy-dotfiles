{
  "env": {},
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "Bash(mkdir:*)",
      "Bash(go:*)",
      "Write(*)",
      "Bash(ls:*)",
      "Bash(git:*)",
      "Update(*:*)",
      "Bash(mv:*)",
      "Bash(echo:*)",
      "Bash(sed:*)",
      "Bash(source:*)",
      "Bash(head:*)",
      "Bash(tail:*)",
      "Bash(npm:*)",
      "Bash(npx:*)",
      "Bash(pkill:*)",
      "Bash(touch:*)",
      "Bash(grep:*)",
      "Bash(deadcode:*)",
      "Bash(sqlite3:*)",
      "Bash(curl:*)",
      "Bash(rg:*)",
      "Bash(chmod:*)",
      "Bash(lsof:*)",
      "Bash(make:*)",
      "Bash(PORT=:*)",
      "Bash(find:*)",
      "Bash(docker:*)",
      "Bash(poetry:*)",
      "Bash(python:*)",
      "Bash(python3:*)",
      "Update(*)",
      "Bash(bun:*)",
      "Bash(uv:*)",
      "Bash(uvx:*)",
      "Bash(pnpm:*)",
      "Bash(cargo:*)",
      "Bash(rustc:*)",
      "Bash(gh:*)",
      "Bash(pwd)",
      "Bash(which:*)",
      "Bash(cat:*)",
      "Bash(cp:*)",
      "Bash(rm:*)",
      "Bash(wc:*)",
      "Bash(date:*)",
      "Bash(systemctl:*)",
      "Bash(journalctl:*)",
      "Bash(stow:*)",
      "Skill(superpowers:brainstorming)",
      "Skill(superpowers:dispatching-parallel-agents)",
      "Skill(superpowers:executing-plans)",
      "Skill(superpowers:finishing-a-development-branch)",
      "Skill(superpowers:receiving-code-review)",
      "Skill(superpowers:requesting-code-review)",
      "Skill(superpowers:subagent-driven-development)",
      "Skill(superpowers:systematic-debugging)",
      "Skill(superpowers:test-driven-development)",
      "Skill(superpowers:using-git-worktrees)",
      "Skill(superpowers:using-superpowers)",
      "Skill(superpowers:verification-before-completion)",
      "Skill(superpowers:writing-plans)",
      "Skill(superpowers:writing-skills)",
      "WebFetch(domain:docs.anthropic.com)",
      "WebFetch(domain:docs.claude.ai)",
      "WebFetch(domain:docs.python.org)",
      "WebFetch(domain:docs.aws.amazon.com)",
      "WebFetch(domain:github.com)",
      "WebFetch(domain:raw.githubusercontent.com)",
      "WebFetch(domain:go.dev)",
      "WebFetch(domain:pkg.go.dev)",
      "WebFetch(domain:wiki.archlinux.org)",
      "WebFetch(domain:wiki.hyprland.org)",
      "WebFetch(domain:payloadcms.com)",
      "WebFetch(domain:supabase.com)",
      "WebFetch(domain:nextjs.org)",
      "WebFetch(domain:tailwindcss.com)",
      "WebFetch(domain:www.npmjs.com)",
      "WebFetch(domain:pypi.org)",
      "WebFetch(domain:stackoverflow.com)",
      "WebSearch"
    ],
    "deny": []
  },
  "hooks": {
    "PostToolUse": [
      {
        "matcher": "Write",
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/post-write-format.sh"
          }
        ]
      },
      {
        "matcher": "Edit",
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/post-write-format.sh"
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "~/.claude/statusline.sh",
    "padding": 0
  },
  "enabledPlugins": {
    "superpowers@superpowers-marketplace": true,
    "superpowers@superpowers-dev": false,
    "superpowers-chrome@superpowers-marketplace": true,
    "frontend-design@claude-plugins-official": true,
    "superpowers-lab@superpowers-marketplace": true,
    "context7@claude-plugins-official": true,
    "serena@claude-plugins-official": true,
    "typescript-lsp@claude-plugins-official": true,
    "learning-output-style@claude-plugins-official": true,
    "sentry@claude-plugins-official": false,
    "commit-commands@claude-plugins-official": true,
    "ralph-wiggum@claude-plugins-official": true,
    "payload@payload-marketplace": true,
    "omarchy-skill@local": true,
    "pyright-lsp@claude-plugins-official": true,
    "vercel@claude-plugins-official": false,
    "hookify@claude-plugins-official": true,
    "double-shot-latte@superpowers-marketplace": true,
    "explanatory-output-style@claude-plugins-official": true,
    "changelog-generator@awesome-claude-skills": true,
    "superpowers@claude-plugins-official": true,
    "ralph-loop@claude-plugins-official": true,
    "ralph-specum@smart-ralph": true
  },
  "extraKnownMarketplaces": {
    "superpowers-dev": {
      "source": {
        "source": "github",
        "repo": "obra/superpowers"
      }
    }
  },
  "alwaysThinkingEnabled": true
}
